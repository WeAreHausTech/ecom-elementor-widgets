{
  "name": "livv-ecom-elementor-widgets",
  "private": true,
  "version": "0.1.21",
  "type": "module",
  "scripts": {
    "build-widgets": "rm -rf ./dist && rm -rf ./.parcel-cache && tsc && parcel build --no-scope-hoist",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@haus-tech/ecom-components": "^0.1.12",
    "classnames": "^2.3.2",
    "lodash": "^4.17.21",
    "parcel": "^2.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@haus-tech/prettier-config": "^1.0.3",
    "@parcel/packager-ts": "^2.10.0",
    "@parcel/transformer-raw": "^2.10.0",
    "@parcel/transformer-sass": "^2.10.0",
    "@parcel/transformer-typescript-types": "^2.10.0",
    "@types/lodash": "^4.14.198",
    "@types/node": "^20.8.6",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "parcel-namer-rewrite": "^2.10.3-rc.1",
    "parcel-reporter-bundle-manifest": "^1.0.0",
    "path": "^0.12.7",
    "postcss": "^8.4.29",
    "sass": "^1.66.1",
    "typescript": "^5.0.2"
  },
  "source": "components/index.tsx",
  "@parcel/resolver-default": {
    "packageExports": false
  },
  "parcel-namer-rewrite": {
    "hashing": "always",
    "rules": {
      "(.*).css": "$1.{hash}.css",
      "(.*).js": "$1.{hash}.js"
    }
  }
}
